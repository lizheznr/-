}
.input-text{
  font-size: 10px;
  height: calc(750rpx * 20 /320);
  color:rgb(197, 188, 188);
  justify-content: flex-end;
}
.viewName{
  display: flex;
  padding-top: calc(750rpx * 20 /320);
}
.loginBtn{
  height: calc(750rpx * 35 /320);
  width:calc(750rpx * 70/320);
  align-items: right;
}
.resetBtn{
  height:calc(750rpx * 35 /320);
  width:calc(750rpx * 70/320);
